{
  "name": "owe-flow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "nvm-setup": "bash -c 'source \"$NVM_DIR/nvm.sh\" && nvm use || nvm install'",
    "dev": "npm run nvm-setup && next dev",
    "build": "npm run nvm-setup && next build",
    "start": "npm run nvm-setup && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,md,mdx,json,yml,css}\" --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "prettier:fix": "prettier \"**/*.{js,jsx,ts,tsx,md,mdx,json,yml,css}\" --write --cache --cache-strategy=content --cache-location=.prettiercache",
    "format": "npm run lint:fix && npm run prettier:fix",
    "prepare": "husky",
    "analyze": "ANALYZE=true next build",
    "analyze:win": "set ANALYZE=true && next build",
    "generate-icons": "node scripts/generate-icon-index.mjs"
  },
  "dependencies": {
    "@bprogress/next": "^3.2.10",
    "@next/bundle-analyzer": "^15.2.5",
    "babel-plugin-react-compiler": "19.0.0-beta-e993439-20250405",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.487.0",
    "next": "15.2.5",
    "next-intl": "^4.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.5",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.29.1"
  }
}
