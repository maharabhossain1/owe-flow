{
  "name": "owe-flow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": " next dev ",
    "build": " next build",
    "start": " next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,md,mdx,json,yml,css}\" --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "prettier:fix": "prettier \"**/*.{js,jsx,ts,tsx,md,mdx,json,yml,css}\" --write --cache --cache-strategy=content --cache-location=.prettiercache",
    "format": "npm run lint:fix && npm run prettier:fix",
    "prepare": "husky",
    "analyze": "ANALYZE=true next build",
    "analyze:win": "set ANALYZE=true && next build",
    "generate-icons": "node scripts/generate-icon-index.mjs",
    "db:test": "node scripts/test-db.mjs",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "db:check": "drizzle-kit check",
    "db:pull": "drizzle-kit pull"
  },
  "dependencies": {
    "@bprogress/next": "^3.2.11",
    "@hookform/resolvers": "^5.0.1",
    "@neondatabase/serverless": "^1.0.0",
    "@next/bundle-analyzer": "15.3.0",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "babel-plugin-react-compiler": "19.0.0-beta-e993439-20250405",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.41.0",
    "lucide-react": "^0.488.0",
    "next": "15.3.0",
    "next-intl": "^4.0.2",
    "pg": "^8.14.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.55.0",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/node": "^22.14.1",
    "@types/pg": "^8.11.12",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.2",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.24.0",
    "eslint-config-next": "15.3.0",
    "eslint-plugin-import-x": "^4.10.3",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.1",
      "@types/react-dom": "19.1.2"
    }
  }
